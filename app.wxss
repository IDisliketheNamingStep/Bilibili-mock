/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix::after,.clearfix::before {
  content: '';
  display: table;
}
.clearfix::after{
  clear: both;
}
.tof2 {
  /* 超过两行显示省略号 */
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  /* 超过两行显示省略号 */
}
